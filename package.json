    "bootstrap-icons": "^1.10.3",
    "cal-heatmap": "^4.2.4",
    "clipboard": "^2.0.11",
    "d3": "~7.9.0",
    "d3-tip": "~0.9.1",
    "jquery": "^3.7.1",
    "local-time": "^3.0.0",
    "popper.js": "^1.16.1",
    "sortablejs": "^1.15.7",
    "urijs": "^1.19.11",
    "vega": "^6.0.0",
    "vega-embed": "^7.1.0",
    "vega-lite": "^6.4.1"

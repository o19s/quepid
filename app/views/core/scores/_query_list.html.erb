<%# Rendered by RunCaseEvaluationJob to broadcast updated query scores via Turbo Streams. %>
<%# Replaces the query list frame when scores change (e.g. after DocFinder bulk rating). %>
<%= render QueryListComponent.new(
  case_id: case_id,
  try_number: try_number,
  queries: queries,
  other_cases: other_cases,
  scorer_scale_max: scorer_scale_max,
  scorer_scale: local_assigns[:scorer_scale],
  scale_with_labels: local_assigns[:scale_with_labels],
  query_scores: query_scores,
  sortable: sortable
) %>
